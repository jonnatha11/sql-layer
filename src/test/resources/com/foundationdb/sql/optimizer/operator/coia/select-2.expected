PhysicalSelect[name:varchar(32), order_date:date, city:varchar(100)]
  Project_Default(customers.name, orders.order_date, addresses.city)
    Map_NestedLoops(0)
      Flatten_HKeyOrdered(customers INNER addresses)
        GroupLookup_Default(Index(addresses.state) -> customers, addresses)
          IndexScan_Default(Index(addresses.state), state = 'MA')
      Product_Nested(customers - addresses x customers - orders)
        Filter_Default(customers - orders)
          Flatten_HKeyOrdered(customers INNER orders)
            BranchLookup_Nested(customers -> orders)